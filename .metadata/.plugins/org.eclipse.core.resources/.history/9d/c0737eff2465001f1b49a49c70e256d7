package _21_AutomorphicNumber;

public class Problem {
	
	public static boolean isAutomorphic(int num) {
		int sqr=num*num;
		
		while(num>0) {
			if(num%10 != sqr%10) {
				num=num/10;
				sqr=sqr/10;
			}
		}
		return true;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
